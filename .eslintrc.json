{
    "plugins": [],
    "rules": {
      "at-rule-empty-line-before": [2, "always"],
      "at-rule-name-case": [2, "lower"],
      "at-rule-name-space-after": [2, "always"],
      "at-rule-no-unknown": [2, { "ignoreAtRules": ["include"] }],
      "at-rule-no-vendor-prefix": [2, 2],
      "at-rule-semicolon-newline-after": [2, "always"],
      "at-rule-semicolon-space-before": [2, "always"],
      "block-closing-brace-newline-after": [2, "always"],
      "block-no-empty": 2,
      "block-opening-brace-space-after": [2, "always-single-line"],
      "block-opening-brace-space-before": [2, "always"],
      "color-hex-case": [2, "lower"],
      "color-hex-length": [2, "short"],
      "color-named": [2, "always-where-possible"],
      "color-no-invalid-hex": 2,
      "comment-empty-line-before": [2, "always"],
      "comment-no-empty": 2,
      "comment-whitespace-inside": [2, "always"],
      "custom-property-empty-line-before": [2, "always"],
      "declaration-bang-space-after": [2, "always"],
      "declaration-bang-space-before": [2, "always"],
      "declaration-block-no-duplicate-properties": 2,
      "declaration-block-no-redundant-longhand-properties": 2,
      "declaration-block-no-shorthand-property-overrides": 2,
      "declaration-block-semicolon-newline-after": [2, "always"],
      "declaration-block-semicolon-newline-before": [2, "never-multi-line"],
      "declaration-block-semicolon-space-after": [2, "always-single-line"],
      "declaration-block-single-line-max-declarations": 1,
      "declaration-block-trailing-semicolon": [2, "always"],
      "declaration-colon-space-after": [2, "always"],
      "declaration-empty-line-before": 2,
      "declaration-no-important": 2,
      "eslint-plugin-eslint-plugin/linebreak-style": ["error", "unix"],
      "font-family-name-quotes": [2, "always-where-required"],
      "font-family-no-duplicate-names": 2,
      "font-family-no-missing-generic-family-keyword": 2,
      "font-weight-notation": [2, "numeric"],
      "function-calc-no-unspaced-operator": 2,
      "function-comma-space-after": [2, "always"],
      "function-comma-space-before": [2, "never"],
      "function-linear-gradient-no-nonstandard-direction": 2,
      "function-max-empty-lines": 0,
      "function-name-case": [2, "lower"],
      "function-parentheses-space-inside": [2, "never"],
      "function-url-no-scheme-relative": 2,
      "function-url-quotes": 2,
      "function-whitespace-after": 2,
      "indentation": [
        2,
        {
          "baseIndentLevel": 1
        }
      ],
      "keyframe-declaration-no-important": 2,
      "length-zero-no-unit": 2,
      "linebreaks": 2,
      "max-empty-lines": 2,
      "max-line-length": [2, 90],
      "max-nesting-depth": 2,
      "media-feature-colon-space-after": [2, "always"],
      "media-feature-colon-space-before": [2, "always"],
      "media-feature-name-case": [2, "lower"],
      "media-feature-name-no-unknown": 2,
      "media-feature-name-no-vendor-prefix": 2,
      "media-feature-parentheses-space-inside": [2, "always"],
      "media-feature-range-operator-space-after": [2, "always"],
      "media-feature-range-operator-space-before": [2, "always"],
      "media-query-list-comma-space-after": [2, "always"],
      "media-query-list-comma-space-before": [2, "never"],
      "no-descending-specificity": 2,
      "no-duplicate-at-import-rules": 2,
      "no-duplicate-selectors": 2,
      "no-empty-source": 2,
      "no-empty-first-line": 2,
      "no-eol-whitespace": 2,
      "no-extra-semicolons": 2,
      "no-invalid-double-slash-comments": 2,
      "no-missing-end-of-source-newline": 2,
      "no-unknown-animations": 2,
      "number-leading-zero": [2, "always"],
      "number-max-precision": [2, 3],
      "number-no-trailing-zeros": 2,
      "order/properties-alphabetical-order": 2,
      "property-case": [2, "lower"],
      "property-no-unknown": 2,
      "property-no-vendor-prefix": 2,
      "rule-empty-line-before": [2, "always", { "except": ["after-single-line-comment"] }],
      "selector-attribute-brackets-space-inside": [2, "always"],
      "selector-attribute-operator-space-after": [2, "always"],
      "selector-attribute-operator-space-before": [2, "always"],
      "selector-attribute-quotes": [2, "always"],
      "selector-combinator-space-after": [2, "always"],
      "selector-combinator-space-before": [2, "always"],
      "selector-descendant-combinator-no-non-space": 2,
      "selector-list-comma-space-after": [2, "always"],
      "selector-list-comma-space-before": [2, "never"],
      "selector-max-attribute": [2, 3],
      "selector-max-class": [2, 5],
      "selector-max-combinators": [2, 3],
      "selector-max-compound-selectors": [2, 5],
      "selector-max-empty-lines": 0,
      "selector-max-id": [2, 5],
      "selector-max-pseudo-class": [2, 3],
      "selector-max-specificity": [2, "0,5,0"],
      "selector-max-type": [2, 5],
      "selector-max-universal": [2, 3],
      "selector-no-qualifying-type": 2,
      "selector-no-vendor-prefix": 2,
      "selector-pseudo-class-case": [2, "lower"],
      "selector-pseudo-class-no-unknown": 2,
      "selector-pseudo-class-parentheses-space-inside": [2, "always"],
      "selector-pseudo-element-case": [2, "lower"],
      "selector-pseudo-element-colon-notation": [2, "single"],
      "selector-pseudo-element-no-unknown": 2,
      "selector-type-case": [2, "lower"],
      "selector-type-no-unknown": 2,
      "shorthand-property-no-redundant-values": 2,
      "string-no-newline": 2,
      "string-quotes": [2, "single"],
      "time-min-milliseconds": 2,
      "unit-case": [2, "lower"],
      "unit-no-unknown": 2,
      "value-keyword-case": [2, "lower"],
      "value-list-comma-space-after": [2, "always"],
      "value-list-comma-space-before": [2, "always"],
      "value-list-max-empty-lines": 0,
      "value-no-vendor-prefix": 2
    }
  }
